{
  "name": "appium-cli",
  "version": "0.0.0",
  "author": "dpgraham @dpgraham",
  "bin": {
    "appium": "./bin/run"
  },
  "bugs": "https://github.com/appium/appium/issues",
  "dependencies": {
    "@oclif/command": "^1.5.4",
    "@oclif/config": "^1.9.0",
    "@oclif/plugin-help": "^2.1.3",
    "appium-support": "^2.11.2",
    "bluebird": "^3.5.3",
    "teen_process": "^1.14.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.4",
    "appium-gulp-plugins": "^3.1.5",
    "globby": "^8.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/build"
  ],
  "homepage": "https://github.com/appium/appium",
  "keywords": [
    "oclif"
  ],
  "license": "ISC",
  "main": "build/lib/index.js",
  "oclif": {
    "commands": "./build/lib/cli/commands",
    "bin": "appium",
